<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="efund_kyc_investor_report_pdf" model="ir.actions.report">
        <field name="name">KYC Investor Report</field>
        <field name="model">efund.investor</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">efund.efund_kyc_report_template</field>
        <field name="report_file">efund.efund_kyc_report_template</field>
        <field name="binding_model_id" ref="model_efund_investor"/>
